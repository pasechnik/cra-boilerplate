{
  "name": "cra-boilerplate",
  "version": "1.0.2",
  "private": true,
  "homepage": "http://appshub.xyz/",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "bootstrap": "^4.1.1",
    "classname": "0.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "libphonenumber-js": "^1.2.15",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "nprogress": "^0.2.0",
    "prettier-eslint": "^8.8.1",
    "prop-types": "^15.6.1",
    "query-string": "^5.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-input-range": "^1.3.0",
    "react-loadable": "^5.3.1",
    "react-phone-number-input": "^1.1.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-snapshot": "^1.3.0",
    "react-test-renderer": "^16.3.2",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-observable": "^0.18.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "socket.io-client": "^2.1.1",
    "styled-components": "^3.2.6",
    "the-utils": "0.5.4"
  },
  "devDependencies": {
    "react-scripts": "1.1.4",
    "redux-devtools": "^3.4.1",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start:all": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build:all": "npm-run-all build-css build-js",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "build:snapshot": "better-npm-run build:snapshot",
    "test": "better-npm-run test:ci",
    "eject": "react-scripts eject",
    "posttest": "npm run lint",
    "test:ci": "better-npm-run test:ci",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test:all": "better-npm-run test:all"
  },
  "betterScripts": {
    "start": {
      "command": "react-scripts start",
      "env": {
        "FOLDER": "root",
        "NODE_ENV": "dev",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "react-scripts build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:snapshot": {
      "command": "react-scripts build && react-snapshot",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test:ci": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "CI": true,
        "DEBUG": "app:*"
      }
    },
    "test:watch": {
      "command": "react-scripts test --env=jsdom",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/blackit/cra-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "create-react-app",
    "react-loadable",
    "react-redux",
    "react-router",
    "reactstrap",
    "redux-observable",
    "boilerplate"
  ],
  "author": {
    "name": "Pasechnik Vladimir",
    "url": "https://github.com/pasechnik"
  },
  "license": "MIT"
}
